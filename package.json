{
  "name": "@mangata-finance/sdk",
  "version": "1.11.4",
  "description": "SDK for communication with Mangata node",
  "main": "./index.js",
  "module": "./index.mjs",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "require": "./index.js",
      "import": "./index.mjs",
      "types": "./index.d.ts"
    }
  },
  "scripts": {
    "prepare": "if [ -z \"$SKIP_HUSKY\" ]; then husky install ; fi",
    "build": "rm -fr build/* && tsc -p tsconfig.json && rollup -c",
    "test": "vitest --config ./vitest.config.ts --run",
    "start": "npm run build && node ./index.js",
    "eslint": "node_modules/.bin/eslint --max-warnings 0 . --ext .ts",
    "eslint-fix": "npm run eslint --fix"
  },
  "files": [
    "index.js",
    "index.mjs",
    "index.d.ts"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "tag": "latest"
  },
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mangata-finance/mangata-SDK.git"
  },
  "keywords": [
    "typescript",
    "mangata",
    "parachain",
    "sdk"
  ],
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "@babel/preset-react"
    ]
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {
      "no-var": "warn",
      "prefer-const": "warn",
      "no-console": "warn",
      "arrow-body-style": "off",
      "prefer-arrow-callback": "off",
      "semi": 0
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release": {
    "branches": [
      "release/*"
    ],
    "repositoryUrl": "https://github.com/mangata-finance/mangata-sdk",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ],
      "@semantic-release/github"
    ]
  },
  "author": "Mangata Finance",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/mangata-finance/mangata-SDK/issues"
  },
  "homepage": "https://github.com/mangata-finance/mangata-SDK#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@types/big.js": "^6.1.3",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "dotenv": "^10.0.0",
    "eslint": "^8.15.0",
    "husky": "^8.0.1",
    "process": "^0.11.10",
    "rollup": "^2.70.2",
    "rollup-plugin-dts": "^4.2.1",
    "rollup-plugin-terser": "^7.0.2",
    "semantic-release": "^19.0.3",
    "ts-node": "^10.5.0",
    "typescript": "^4.7.4",
    "vitest": "^0.25.6"
  },
  "dependencies": {
    "@mangata-finance/types": "^0.17.0",
    "@polkadot/api": "^9.10.1",
    "big.js": "^6.2.1",
    "mangata-prng-xoshiro": "^3.0.0",
    "uuid": "^9.0.0"
  }
}
