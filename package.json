{
  "name": "mangata-sdk",
  "version": "0.18.5",
  "description": "SDK for communication with Mangata node",
  "main": "build/cjs/index.js",
  "module": "build/mjs/index.js",
  "types": "build/cjs/index.d.ts",
  "exports": {
    ".": {
      "import": "./build/mjs/index.js",
      "require": "./build/cjs/index.js"
    }
  },
  "scripts": {
    "build": "rm -fr build/* && tsc -p tsconfig.json && tsc -p tsconfig-cjs.json && ./fixup",
    "test": "jest --runInBand --detectOpenHandles --verbose",
    "start": "yarn build && node ./build/index.js",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "eslint": "node_modules/.bin/eslint --max-warnings 0 . --ext .ts",
    "eslint-fix": "yarn eslint --fix",
    "bump:major": "standard-version --release-as major",
    "bump:minor": "standard-version --release-as minor",
    "bump:patch": "standard-version --release-as patch",
    "buildTypes": "yarn generate:defs && yarn generate:meta",
    "generate:defs": "ts-node --skip-project node_modules/.bin/polkadot-types-from-defs --package mangata-sdk/interfaces --input ./src/interfaces --endpoint ./mangata-metadata.json",
    "generate:meta": "ts-node --skip-project node_modules/.bin/polkadot-types-from-chain --package mangata-sdk/interfaces --endpoint ./mangata-metadata.json --output ./src/interfaces"
  },
  "files": [
    "build"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mangata-finance/mangata-SDK.git"
  },
  "keywords": [
    "typescript",
    "mangata",
    "node",
    "sdk"
  ],
  "author": "Mangata Finance",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/mangata-finance/mangata-SDK/issues"
  },
  "homepage": "https://github.com/mangata-finance/mangata-SDK#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@polkadot/typegen": "^7.10.1",
    "@types/big.js": "^6.1.2",
    "@types/jest": "^26.0.23",
    "@types/jest-when": "^2.7.3",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.0.5",
    "dotenv": "^10.0.0",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.5",
    "jest-when": "^3.3.1",
    "prettier": "^2.3.2",
    "process": "^0.11.10",
    "standard-version": "^9.3.1",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "@polkadot/api": "^7.10.1",
    "big.js": "^6.1.1",
    "bn.js": "^5.2.0",
    "mangata-prng-xoshiro": "^1.2.0",
    "uuid": "^8.3.2"
  }
}
