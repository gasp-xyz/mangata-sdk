{
  "name": "@mangata-finance/sdk",
  "version": "1.9.0",
  "description": "SDK for communication with Mangata node",
  "main": "./index.js",
  "module": "./index.mjs",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "require": "./index.js",
      "import": "./index.mjs",
      "types": "./index.d.ts"
    }
  },
  "scripts": {
    "prepare": "if [ -z \"$SKIP_HUSKY\" ]; then husky install ; fi",
    "build": "rm -fr build/* && tsc -p tsconfig.json && rollup -c",
    "test": "vitest --config ./vitest.config.ts --run",
    "start": "npm run build && node ./index.js",
    "eslint": "node_modules/.bin/eslint --max-warnings 0 . --ext .ts",
    "eslint-fix": "npm run eslint --fix"
  },
  "files": [
    "index.js",
    "index.mjs",
    "index.d.ts"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "tag": "latest"
  },
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mangata-finance/mangata-SDK.git"
  },
  "keywords": [
    "typescript",
    "mangata",
    "parachain",
    "sdk"
  ],
  "author": "Mangata Finance",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/mangata-finance/mangata-SDK/issues"
  },
  "homepage": "https://github.com/mangata-finance/mangata-SDK#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@types/big.js": "^6.1.3",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "dotenv": "^10.0.0",
    "eslint": "^8.15.0",
    "husky": "^8.0.1",
    "process": "^0.11.10",
    "rollup": "^2.70.2",
    "rollup-plugin-dts": "^4.2.1",
    "rollup-plugin-terser": "^7.0.2",
    "semantic-release": "^19.0.3",
    "ts-node": "^10.5.0",
    "typescript": "^4.7.4",
    "vitest": "^0.22.1"
  },
  "dependencies": {
    "@mangata-finance/types": "^0.9.0",
    "@polkadot/api": "^9.6.2",
    "big.js": "^6.1.1",
    "mangata-prng-xoshiro": "^3.0.0",
    "uuid": "^8.3.2"
  }
}
